import request from '@/utils/request'
import setting from '@/settings'
import qs from 'qs'
const apiUrl = setting.apiUrl

/**
* 栏目列表
* @param data
*/
export const catalogList = (data: any) =>
  request({
    url: `${apiUrl.api}/fcs/noticeCatalog/list`,
    method: 'post',
    data: qs.stringify(data)
  })

