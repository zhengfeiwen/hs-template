import request from '@/utils/request'
import setting from '@/settings'
import qs from 'qs'
const apiUrl = setting.apiUrl

/**
* 获取业务列表
* @param data
*/
export const businessList = (data: any) =>
  request({
    url: `${apiUrl.api}/fcs/business/search`,
    method: 'post',
    data: qs.stringify(data.data)
  })

