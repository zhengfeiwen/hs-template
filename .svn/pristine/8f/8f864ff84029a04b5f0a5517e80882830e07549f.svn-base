.title{
    margin: 10px 0;
    display: flex;
}
.title>span{
    font-size: 20px;
    font-weight: bold;
    margin-right: 10px;
}

.simple-table{
    border: 1px solid #d9d9d9;
    border-collapse: collapse;
    border-spacing: 0;
    width: 100%;
}

.simple-table tr td{
    height: 50px;
    padding: 0;
    width: 50%;
}

