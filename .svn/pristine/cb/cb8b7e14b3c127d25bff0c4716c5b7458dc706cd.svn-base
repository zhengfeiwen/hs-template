.abnormal-turn-record-dialog-container{
    .ul-info{
        display: flex;
        list-style:none;
        li{
            margin-right: 30px;
            color: #606266;
            font-weight: bold;
            font-size: 14px;
            span{
                color: #333;
                font-weight: normal;
            }
        }
    }
    .title-2{
        margin-top: 30px;
        color: #333;
    }
    .ul-money{
        padding-left: 0;
        display: flex;
        list-style:none;
        justify-content: space-between;
        li{
            margin-right: 30px;
            color: #606266;
            font-weight: bold;
            font-size: 14px;
            span{
                color: #333;
                font-weight: normal;
            }
        }
    }
    .el-collapse{
        border: 0;
    }
    .el-collapse-item__header{
        font-weight: bold;
        font-size: 20px;
    }
    .line {
        width: 100%;
        display: flex;
        align-items: center;
        margin-bottom: 12px;
        .hs-icon-user {
            font-size: 22px;
            font-weight: bold;
            margin-right: 12px;
            margin-top: 2px;
        }
        .student-name {
            font-size: 20px;
            font-weight: bold;
        }
        &.auto .static-label{
          font-size: 16px;
          font-weight: bold;
        }
    }
}