$leftWidth: 120px;
.refund-manage-container{
  height: 100%;
  width: 100%;
  display: flex;
  .left{
    margin-left: -20px;
    width: $leftWidth;
    height: 100%;
  }
  .right{
    width: calc(100% - #{$leftWidth} - 10px);
    height: 100%;
    padding-left: 10px;
    header{
      width: 100%;
      .header{
        width: 100%;
        display: flex;
        justify-content: space-between;
        align-items: center;
        span.title{
          font-size: 18px;
          font-weight: bold;
        }
      }
    }
  }
  .export-body{
    display: none;
  }
}