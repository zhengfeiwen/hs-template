import request from '@/utils/request'
import setting from '@/settings'
const apiUrl = setting.apiUrl

/**
* 删除用户
* @param data
*/
export const removeUser = (data: any) =>
  request({
    url: `${apiUrl.api}/fcs/user/delete`,
    method: 'post',
    data
  })

